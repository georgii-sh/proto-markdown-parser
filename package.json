{
  "name": "@protomarkdown/parser",
  "version": "1.0.0",
  "type": "module",
  "description": "Parser and renderer for proto markdown syntax",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/georgii-sh/proto-markdown-parser.git"
  },
  "keywords": [
    "Protomarkdown",
    "UI",
    "prototyping",
    "markdown"
  ],
  "author": "Georgii Sh <georgii.sh@hotmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/georgii-sh/proto-markdown-parser/issues"
  },
  "homepage": "https://github.com/georgii-sh/proto-markdown-parser#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.8",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.1.4",
    "rollup": "^4.52.5",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
